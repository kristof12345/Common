<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk.Razor">
	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<PackOnBuild>true</PackOnBuild>
		<PackageId>Waikiki.Common.Frontend</PackageId>
		<Authors>Papp Kristóf</Authors>
		<Copyright>All rights reserved.</Copyright>
		<PackageLicense>https://github.com/kristof12345/Common/blob/master/LEGAL.md</PackageLicense>
		<Owners>Papp Kristóf</Owners>
		<PackageProjectUrl>https://github.com/kristof12345/Common</PackageProjectUrl>
		<PackageVersion>2.8.5</PackageVersion>
		<Description>Common components for building Blazor frontend applications.</Description>
	</PropertyGroup>
	<ItemGroup>
		<SupportedPlatform Include="browser" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\..\Common\Application\Common.Application.csproj" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="6.0.6" />
		<PackageReference Include="Microsoft.AspNetCore.SignalR.Client" Version="6.0.6" />
		<PackageReference Include="Blazored.LocalStorage" Version="4.2.0" />
		<PackageReference Include="Blazored.SessionStorage" Version="2.2.0" />
		<PackageReference Include="Syncfusion.Blazor" Version="18.3.0.53" />
	</ItemGroup>
	<ItemGroup>
		<Content Remove="wwwroot\.DS_Store" />
		<Content Remove="wwwroot\styles\.DS_Store" />
		<Content Remove="wwwroot\images\.DS_Store" />
		<Content Remove="wwwroot\scripts\.DS_Store" />
		<Content Remove="wwwroot\fonts\.DS_Store" />
	</ItemGroup>
</Project>